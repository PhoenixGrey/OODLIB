li(ng-class='model.rawData.klass')
  a.nav-item.dropdown-toggle(href="#" ng-click="toggleSelect()")
    i(ng-class='model.rawData.icon')
  .dropdown-menu(ng-class="{'is-show':isPopupVisible}")
    .dropdown-menu-header.li-border-bottom
      | {{model.rawData.name}}
    ul.height-control
      li.li-border-bottom(ng-repeat='child in model.rawData.subNodes')
        .fa-text-indent
          i(ng-class="child.icon")
          span
            | {{child.name}}
      li
        .fa-text-indent
          i(ng-class="model.rawData.privateCloud.icon")
          span
            | {{model.rawData.privateCloud.name}}
          i.fa.fa-angle-down.is-align-right#chevron-down-ctrl2.fa-icon-angle(ng-click="subToggleSelect()")
        ul#chevron-down2.dropdown-menu-sub(ng-class="{'is-show':subToggle}")
          li.li-border-bottom(ng-repeat="info in model.rawData.privateCloud.info")
            .fa-text-indent
              i(ng-class="info.icon")
              span.title
                | {{info.name}}
            .fa-text-indent(ng-repeat="v in info.value")
              span
                | {{v}}
        li.dropdown-menu-footer
          .fa-text-indent(ng-repeat='info in model.rawData.footer.info')
            i.fa.fa-tag.fa-flip-horizontal.fa-left-icon
            span
              | {{info}}
